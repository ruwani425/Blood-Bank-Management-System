            <JFXButton id="bevel" fx:id="deleteCampaignBtn" onAction="#deleteCampaignOnAction" prefHeight="30.0" prefWidth="130.0" stylesheets="@../lk/ijse/gdse/bbms/styleSheets/buttonsCss2.css" text="Delete" />
